<?xml version="1.0" encoding="utf-8" ?>
<AppEnvCollections Version="1.0">
    <!--AppName即是执行程序的名字,不区分大小写-->
    <!--Version是右键属性，看到的“产品版本”，可以为空，表示匹配所有版本，可以加逗号或星号
  <Version></Version>-->
    <!--
  特殊路径表示：
  {SystemPath}：例如：C:\
  {UserRootPath}: 例如：C:\Users\
  {CurUserRootPath}: 例如：C:\Users\Administrator\
  {ExePath}: 可运行程序目录，例如：D:\Program Files (x86)\Vim\vim74
  {WorkspacePath}:工作空间目录，例如：C:\Users\Administrator\workspace\
  {EnvConfigRootPath} : 个性化配置路径， 例如：F:\myprofile\EnvConfig
  -->
    <AppEnvCollection>
        <AppName>gvim</AppName>
        <ExePath>D:\myProLocalTools\VIN\vim\vim74\gvim.exe</ExePath>
    </AppEnvCollection>
    <AppEnvCollection>
        <AppName>notepad++</AppName>
        <ExePath>D:\myProLocalTools\npp\Notepad++\notepad++.exe</ExePath>
    </AppEnvCollection>
</AppEnvCollections>
